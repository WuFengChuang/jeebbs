<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.plug.live.entity">
	<class
		name="BbsLiveUser"
		table="bbs_live_user"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="sequence"><param name="sequence">S_BBS_LIVE_USER</param></generator>
		</id>
		<property
			name="buyTime"
			column="buy_time"
			type="timestamp"
			not-null="true"
			length="19"
		/>
		<property
			name="joinTime"
			column="join_time"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<many-to-one
			name="live"
			column="live_id"
			class="BbsLive"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="joinUser"
			column="user_id"
			class="com.jeecms.bbs.entity.BbsUser"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="order"
			column="order_id"
			class="com.jeecms.bbs.entity.BbsOrder"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>