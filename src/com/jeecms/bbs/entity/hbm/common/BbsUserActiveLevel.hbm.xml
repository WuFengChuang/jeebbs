<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.bbs.entity">
	<class
		name="BbsUserActiveLevel"
		table="bbs_user_active_level"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="level_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="levelName"
			column="level_name"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="requiredHour"
			column="required_hour"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="levelImg"
			column="level_img"
			type="string"
			not-null="false"
			length="255"
		/>


	</class>	
</hibernate-mapping>